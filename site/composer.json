{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [{
        "name": "",
        "role": ""
    }],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "jquery-validation": {
            "type": "package",
            "package": {
                "name": "jquery-validation/jquery-validation",
                "version": "1.15.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/jquery-validation/jquery-validation",
                    "type": "git",
                    "reference": "1.15.0"
                }
            }
        },
        "moment": {
            "type": "package",
            "package": {
                "name": "moment/moment",
                "version": "2.17.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/moment/moment",
                    "type": "git",
                    "reference": "2.17.1"
                }
            }
        },
        "fullcalendar": {
            "type": "package",
            "package": {
                "name": "arshaw/fullcalendar",
                "version": "3.1.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/arshaw/fullcalendar",
                    "type": "git",
                    "reference": "v3.1.0"
                }
            }
        },
        "tooltipster": {
            "type": "package",
            "package": {
                "name": "iamceege/tooltipster",
                "version": "4.2.6",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/iamceege/tooltipster/",
                    "type": "git",
                    "reference": "4.2.6"
                }
            }
        }
    },
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/apply_for_role": "^1.0",
        "drupal/appointments": "^1.0",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/better_messages": "^1.0",
        "drupal/block_exclude_pages": "^1.3",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/devel": "^2.1",
        "drupal/devel_entity_updates": "^1.0",
        "drupal/entity": "^1.0",
        "drupal/geocoder": "^2.2",
        "drupal/geofield": "^1.3",
        "drupal/inline_entity_form": "^1.0",
        "drupal/mail_login": "^1.0",
        "drupal/menu_item_role_access": "^1.0",
        "drupal/metatag": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/multiple_registration": "^2.84",
        "drupal/private_message": "^1.2",
        "drupal/profile": "^1.0",
        "drupal/range": "^1.1",
        "drupal/redirect_after_login": "^2.5",
        "drupal/registration_role_with_approval": "^1.0",
        "drupal/token": "^1.5",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },

        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
